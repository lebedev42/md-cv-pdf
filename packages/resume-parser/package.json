    "dev": "tsup --watch",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "ejs": "^3.1.10",
    "playwright": "^1.57.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cv/tsconfig": "workspace:*",
    "@types/ejs": "^3.1.5",
    "@types/node": "^22.10.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
